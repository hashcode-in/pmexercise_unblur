{
    "manifest_version": 3,
    "name": "Modify Web Content",
    "version": "1.0",
    "description": "Removes specific divs and modifies content structure.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  